@version "1";

@target [{
    "path": {
        "start": "recipe/",
        "end": "_trapdoor.json"
    }
}];

if ($.type == "minecraft:crafting_shaped" && "result" in $ && strings.endsWith($result.id, "_trapdoor") && !strings.startsWith($result.id, "blockus:") && $result.count == 2) {
    $result.count = 12;
};